<% provide(:title, 'All Ratings') %>
<h1>New Ratings</h1>

<%= form_with(model: @rating, local: true) do |f| %>
    <%= f.label :meeting_date %>
    <%= date_field(:user, :born_on) %>

    <%= f.label :rating %>
    <h5>Rate from 1 to 10</h5>
    <%= range_field(:content, in: 1..10) %>

   

    <%= f.submit "Rate!", class: "btn btn-primary" %>

<% end %>