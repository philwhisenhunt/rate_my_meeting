<% provide(:title, 'All Ratings') %>
<h1>New Ratings</h1>

<%= form_with do |f| %>
    <%= f.label :meeting_date %>
    <%= date_field(:user, :born_on) %>

    <%= f.label :rating %>
    <h5>Rate from 1 to 10</h5>
    <%= range_field(:rating, :rating, in: 1..10) %>

    <%= submit_tag "Submit Rating" %>

<% end %>